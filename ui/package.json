{
  "name": "ch-logzy",
  "version": "0.0.1",
  "description": "clickhouse logs viewer",
  "main": "public/js/index.js",
  "scripts": {
    "cleanup": "rm -rf .cache dist",
    "watch": "npm run cleanup && parcel watch --no-hmr --no-cache public/index.html",
    "build": "parcel build public/index.html",
    "serve": "parcel serve --no-hmr public/index.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pechorin/ch-logzy.git"
  },
  "author": "andrey pechorin",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/pechorin/ch-logzy/issues"
  },
  "homepage": "https://github.com/pechorin/ch-logzy#readme",
  "devDependencies": {
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-custom-dist-structure": "^1.0.2",
    "sass": "^1.26.3"
  },
  "dependencies": {
    "vue": "^2.6.11"
  },
  "alias": {
    "vue": "./node_modules/vue/dist/vue.common.js"
  },
  "customDistStructure": {
    "config": {
      ".js": "js",
      "images": [
        ".jpg",
        ".png"
      ]
    },
    "options": {
      "development": true
    }
  }
}
